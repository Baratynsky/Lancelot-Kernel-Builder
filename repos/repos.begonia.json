[
  {
    "kernelName": "Astera",
    "kernelSource": {
      "device": "begonia",
      "defconfig": "begonia_user_defconfig",
      "astera": {
        "name": "Astera",
        "repo": "https://github.com/xiaomi-begonia-dev/android_kernel_xiaomi_mt6785",
        "branch": "fifteen"
      },
      "power": {
        "name": "Power",
        "repo": "https://github.com/Saikrishna1504/kernel_xiaomi_mt6785",
        "branch": "vic"
      },
      "powerfork": {
        "name": "PowerFork",
        "repo": "https://github.com/wodanesdag/android_kernel_xiaomi_mt6785",
        "branch": "susfs"
      },
      "nova": {
        "name": "Nova",
        "repo": "https://github.com/Nova-Kernels/kernel_xiaomi_mt6785",
        "branch": "fifteen"
      }
    },
    "withKernelSU": true,
    "withSUSFS": true,
    "toolchains": {
      "default": [
        {
          "repo": "https://gitlab.com/PixelOS-Devices/playgroundtc",
          "branch": "17",
          "name": "clang-playground",
          "binPath": ["./bin"]
        }
      ],
      "powerfork": [
        {
          "repo": "https://gitlab.com/crdroidandroid/android_prebuilts_clang_host_linux-x86_clang-r536225",
          "branch": "15.0",
          "name": "clang-crDroid",
          "binPath": ["./bin"]
        }
      ]
    },
    "ccache": true,
    "params": {
      "default": {
        "ARCH": "arm64",
        "CC": "clang-playground/bin/clang",
        "externalCommand": {
          "CROSS_COMPILE": "clang-playground/bin/aarch64-linux-gnu-",
          "CROSS_COMPILE_ARM32": "clang-playground/bin/arm-linux-gnueabi-",
          "LD": "clang-playground/bin/ld.lld",
          "LLVM": "1",
          "LLVM_IAS": "1",
          "AR": "clang-playground/bin/llvm-ar",
          "NM": "clang-playground/bin/llvm-nm",
          "OBJCOPY": "clang-playground/bin/llvm-objcopy",
          "OBJDUMP": "clang-playground/bin/llvm-objdump",
          "READELF": "clang-playground/bin/llvm-readelf",
          "OBJSIZE": "clang-playground/bin/llvm-size",
          "STRIP": "clang-playground/bin/llvm-strip",
          "LDGOLD": "clang-playground/bin/aarch64-linux-gnu-ld.gold",
          "LLVM_AR": "clang-playground/bin/llvm-ar",
          "LLVM_DIS": "clang-playground/bin/llvm-dis"
        }
      },
      "powerfork": {
        "ARCH": "arm64",
        "CC": "clang-crDroid/bin/clang",
        "externalCommand": {
          "KBUILD_BUILD_HOST": "LEGION",
          "KBUILD_BUILD_USER": "wodanesdag",
          "CROSS_COMPILE": "clang-crDroid/bin/aarch64-linux-gnu-",
          "CROSS_COMPILE_ARM32": "clang-crDroid/bin/arm-linux-gnueabi-",
          "LD": "clang-crDroid/bin/ld.lld",
          "LLVM": "1",
          "LLVM_IAS": "1",
          "AR": "clang-crDroid/bin/llvm-ar",
          "NM": "clang-crDroid/bin/llvm-nm",
          "OBJCOPY": "clang-crDroid/bin/llvm-objcopy",
          "OBJDUMP": "clang-crDroid/bin/llvm-objdump",
          "READELF": "clang-crDroid/bin/llvm-readelf",
          "OBJSIZE": "clang-crDroid/bin/llvm-size",
          "STRIP": "clang-crDroid/bin/llvm-strip",
          "LDGOLD": "clang-crDroid/bin/aarch64-linux-gnu-ld.gold",
          "LLVM_AR": "clang-crDroid/bin/llvm-ar",
          "LLVM_DIS": "clang-crDroid/bin/llvm-dis"
        }
      }
    },
    "AnyKernel3": {
      "use": true,
      "release": true,
      "repo": "https://github.com/cvnertnc/AnyKernel3/",
      "branch": "begonia"
    }
  }
]
